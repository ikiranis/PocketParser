<AnchorPane prefHeight="630.0" prefWidth="1217.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.apps4net.controller.MainController">
   <children>
      <Pane layoutX="14.0" layoutY="14.0" prefHeight="601.0" prefWidth="1189.0">
         <children>
            <TextArea fx:id="bookmarksTextArea" layoutX="21.0" layoutY="80.0" prefHeight="507.0" prefWidth="1148.0">
               <effect>
                  <InnerShadow />
               </effect></TextArea>
            <FlowPane alignment="TOP_RIGHT" columnHalignment="CENTER" hgap="10.0" layoutX="14.0" layoutY="21.0" prefHeight="28.0" prefWidth="1148.0">
